LEFT JOIN ActionProperty_Integer f_age ON f_age.id = (
	SELECT MAX(_ap.id)
	FROM ActionProperty _ap
	INNER JOIN ActionPropertyType _apt ON _apt.id = _ap.type_id AND _apt.code = 'age'
	WHERE _ap.deleted = 0
	AND _ap.action_id = an_father.id
	LIMIT 1
)
LEFT JOIN ActionProperty_Integer f_infertility ON f_infertility.id = (
	SELECT MAX(_ap.id)
	FROM ActionProperty _ap
	INNER JOIN ActionPropertyType _apt ON _apt.id = _ap.type_id AND _apt.code = 'infertility'
	WHERE _ap.deleted = 0
	AND _ap.action_id = an_father.id
	LIMIT 1
)
LEFT JOIN ActionProperty_Integer f_infertility_period ON f_infertility_period.id = (
	SELECT MAX(_ap.id)
	FROM ActionProperty _ap
	INNER JOIN ActionPropertyType _apt ON _apt.id = _ap.type_id AND _apt.code = 'infertility_period'
	WHERE _ap.deleted = 0
	AND _ap.action_id = an_father.id
	LIMIT 1
)
LEFT JOIN ActionProperty_Integer f_alcohol ON f_alcohol.id = (
	SELECT MAX(_ap.id)
	FROM ActionProperty _ap
	INNER JOIN ActionPropertyType _apt ON _apt.id = _ap.type_id AND _apt.code = 'alcohol'
	WHERE _ap.deleted = 0
	AND _ap.action_id = an_father.id
	LIMIT 1
)

LEFT JOIN ActionProperty_Integer f_smoking ON f_smoking.id = (
	SELECT MAX(_ap.id)
	FROM ActionProperty _ap
	INNER JOIN ActionPropertyType _apt ON _apt.id = _ap.type_id AND _apt.code = 'smoking'
	WHERE _ap.deleted = 0
	AND _ap.action_id = an_father.id
	LIMIT 1
)

LEFT JOIN ActionProperty_Integer f_toxic ON f_toxic.id = (
	SELECT MAX(_ap.id)
	FROM ActionProperty _ap
	INNER JOIN ActionPropertyType _apt ON _apt.id = _ap.type_id AND _apt.code = 'toxic'
	WHERE _ap.deleted = 0
	AND _ap.action_id = an_father.id
	LIMIT 1
)

LEFT JOIN ActionProperty_Integer f_drugs ON f_drugs.id = (
	SELECT MAX(_ap.id)
	FROM ActionProperty _ap
	INNER JOIN ActionPropertyType _apt ON _apt.id = _ap.type_id AND _apt.code = 'drugs'
	WHERE _ap.deleted = 0
	AND _ap.action_id = an_father.id
	LIMIT 1
)

WHERE evt.id = $P{event_id}
AND evt.deleted = 0]]>
