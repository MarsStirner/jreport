	<queryString>
		<![CDATA[select

  f_blood_type.name as f_blood_type,
  f_education.value as f_education,
  f_infertility_type.value as f_infertility_type,
  GROUP_CONCAT(DISTINCT f_infertility_treatment.value) as f_infertility_treatment,
  GROUP_CONCAT(DISTINCT f_infertility_cause.value) as f_infertility_cause,
  GROUP_CONCAT(DISTINCT f_hereditary.value) as f_hereditary,
  f_professional_properties.value as f_professional_properties,
  f_work_group.value as f_work_group,
  f_infertility.value as f_infertility,
  f_HIV.value as f_HIV,
  f_toxic.value as f_toxic,
  f_drugs.value as f_drugs,
  f_alcohol.value as f_alcohol,
  f_smoking.value as f_smoking,
  f_age.value as f_age,
  f_infertility_period.value as f_infertility_period,
  f_name.value as f_name,
  f_phone.value as f_phone,
  f_fluorography.value as f_fluorography,
  f_finished_diseases_text.value as f_finished_diseases_text,
  f_current_diseases_text.value as f_current_diseases_text,
  f_finished_diseases.diag_list as f_finished_diseases,
  f_current_diseases.diag_list as f_current_diseases

FROM Event evt
LEFT JOIN Client clnt ON evt.client_id = clnt.id AND clnt.deleted = 0

LEFT JOIN `Action` an_father ON (
        an_father.event_id = evt.id AND
        an_father.actionType_id IN (SELECT id FROM ActionType WHERE ActionType.flatCode = 'risar_father_anamnesis') AND
        an_father.deleted = 0
)

LEFT JOIN (
	SELECT
	GROUP_CONCAT(MKB.DiagID separator ", ") as diag_list,
	_ap.action_id as action_id
	FROM ActionProperty _ap
	INNER JOIN ActionPropertyType _apt ON _apt.id = _ap.type_id AND _apt.code = 'finished_diseases'
	INNER JOIN ActionProperty_MKB _ap_v ON _ap_v.id = _ap.id
	INNER JOIN MKB ON _ap_v.value = MKB.id
	WHERE _ap.deleted = 0
	GROUP BY _ap.action_id
) as f_finished_diseases ON f_finished_diseases.action_id = an_father.id

LEFT JOIN (
	SELECT
	GROUP_CONCAT(MKB.DiagID separator ", ") as diag_list,
	_ap.action_id as action_id
	FROM ActionProperty _ap
	INNER JOIN ActionPropertyType _apt ON _apt.id = _ap.type_id AND _apt.code = 'current_diseases'
	INNER JOIN ActionProperty_MKB _ap_v ON _ap_v.id = _ap.id
	INNER JOIN MKB ON _ap_v.value = MKB.id
	WHERE _ap.deleted = 0
	GROUP BY _ap.action_id
) as f_current_diseases ON f_current_diseases.action_id = an_father.id


LEFT JOIN rbBloodType f_blood_type ON f_blood_type.id = (
	SELECT _api.value
	FROM ActionProperty_Integer _api
	INNER JOIN ActionProperty _ap ON _api.id = _ap.id
	INNER JOIN ActionPropertyType _apt ON _apt.id = _ap.type_id AND _apt.code = 'blood_type'
	WHERE _ap.deleted = 0
	AND _ap.action_id = an_father.id
	LIMIT 1
)

LEFT JOIN ActionProperty_String f_name ON f_name.id = (
	SELECT MAX(_ap.id)
	FROM ActionProperty _ap
	INNER JOIN ActionPropertyType _apt ON _apt.id = _ap.type_id AND _apt.code = 'name'
	WHERE _ap.deleted = 0
	AND _ap.action_id = an_father.id
	LIMIT 1
)
LEFT JOIN ActionProperty_String f_phone ON f_phone.id = (
	SELECT MAX(_ap.id)
	FROM ActionProperty _ap
	INNER JOIN ActionPropertyType _apt ON _apt.id = _ap.type_id AND _apt.code = 'phone'
	WHERE _ap.deleted = 0
	AND _ap.action_id = an_father.id
	LIMIT 1
)
LEFT JOIN ActionProperty_String f_fluorography ON f_fluorography.id = (
	SELECT MAX(_ap.id)
	FROM ActionProperty _ap
	INNER JOIN ActionPropertyType _apt ON _apt.id = _ap.type_id AND _apt.code = 'fluorography'
	WHERE _ap.deleted = 0
	AND _ap.action_id = an_father.id
	LIMIT 1
)
LEFT JOIN ActionProperty_String f_finished_diseases_text ON f_finished_diseases_text.id = (
	SELECT MAX(_ap.id)
	FROM ActionProperty _ap
	INNER JOIN ActionPropertyType _apt ON _apt.id = _ap.type_id AND _apt.code = 'finished_diseases_text'
	WHERE _ap.deleted = 0
	AND _ap.action_id = an_father.id
	LIMIT 1
)
LEFT JOIN ActionProperty_String f_current_diseases_text ON f_current_diseases_text.id = (
	SELECT MAX(_ap.id)
	FROM ActionProperty _ap
	INNER JOIN ActionPropertyType _apt ON _apt.id = _ap.type_id AND _apt.code = 'current_diseases_text'
	WHERE _ap.deleted = 0
	AND _ap.action_id = an_father.id
	LIMIT 1
)

LEFT JOIN ActionProperty_ExtRef f_education ON f_education.id = (
  SELECT MAX(_ap.id)
  FROM ActionProperty _ap
  INNER JOIN ActionPropertyType _apt ON _apt.id = _ap.type_id AND _apt.code = 'education'
  WHERE _ap.deleted = 0
  AND _ap.action_id = an_father.id
  LIMIT 1
