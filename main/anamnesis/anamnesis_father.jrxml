LEFT JOIN ActionProperty_ExtRef f_work_group ON f_work_group.id = (
  SELECT MAX(_ap.id)
  FROM ActionProperty _ap
  INNER JOIN ActionPropertyType _apt ON _apt.id = _ap.type_id AND _apt.code = 'work_group'
