LEFT JOIN ActionProperty_Integer f_HIV ON f_HIV.id = (
	SELECT MAX(_ap.id)
	FROM ActionProperty _ap
	INNER JOIN ActionPropertyType _apt ON _apt.id = _ap.type_id AND _apt.code = 'HIV'
	WHERE _ap.deleted = 0
	AND _ap.action_id = an_father.id
	LIMIT 1
