LEFT JOIN ActionProperty_ExtRef f_infertility_type ON f_infertility_type.id = (
  SELECT MAX(_ap.id)
  FROM ActionProperty _ap
  INNER JOIN ActionPropertyType _apt ON _apt.id = _ap.type_id AND _apt.code = 'infertility_type'
  WHERE _ap.deleted = 0
  AND _ap.action_id = an_father.id
  LIMIT 1
)
LEFT JOIN ActionProperty_ExtRef f_infertility_treatment ON f_infertility_treatment.id = (
  SELECT MAX(_ap.id)
  FROM ActionProperty _ap
  INNER JOIN ActionPropertyType _apt ON _apt.id = _ap.type_id AND _apt.code = 'infertility_treatment'
  WHERE _ap.deleted = 0
  AND _ap.action_id = an_father.id
  LIMIT 1
)
LEFT JOIN ActionProperty_ExtRef f_infertility_cause ON f_infertility_cause.id = (
  SELECT MAX(_ap.id)
  FROM ActionProperty _ap
  INNER JOIN ActionPropertyType _apt ON _apt.id = _ap.type_id AND _apt.code = 'infertility_cause'
  WHERE _ap.deleted = 0
  AND _ap.action_id = an_father.id
  LIMIT 1
