LEFT JOIN ActionProperty_ExtRef f_professional_properties ON f_professional_properties.id = (
  SELECT MAX(_ap.id)
  FROM ActionProperty _ap
  INNER JOIN ActionPropertyType _apt ON _apt.id = _ap.type_id AND _apt.code = 'professional_properties'
  WHERE _ap.deleted = 0
