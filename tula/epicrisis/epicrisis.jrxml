	_ap.action_id as action_id
	FROM ActionProperty _ap
	INNER JOIN ActionPropertyType _apt ON _apt.id = _ap.type_id AND _apt.code = 'operation_complication'
	INNER JOIN ActionProperty_MKB _ap_v ON _ap_v.id = _ap.id
	INNER JOIN MKB ON _ap_v.value = MKB.id
	WHERE _ap.deleted = 0
	GROUP BY _ap.action_id
) as operationComplication ON operationComplication.action_id = epicrisis.id

WHERE
epicrisis.deleted = 0
AND event.deleted = 0

AND epicrisis.id = $P{action_id}

