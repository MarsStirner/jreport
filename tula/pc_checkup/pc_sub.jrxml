  INNER JOIN ActionPropertyType _apt ON _apt.id = _ap.type_id AND _apt.code = 'vagina'
  WHERE _ap.deleted = 0
  AND _apt.deleted = 0
  AND _ap.action_id = a.id
  LIMIT 1
)
-- Наружные половые органы
LEFT JOIN ActionProperty_String externalia ON externalia.id= (
  SELECT _ap.id
  FROM ActionProperty _ap
  INNER JOIN ActionPropertyType _apt ON _apt.id = _ap.type_id AND _apt.code = 'externalia'
  WHERE _ap.deleted = 0
  AND _apt.deleted = 0
  AND _ap.action_id = a.id
  LIMIT 1
)

WHERE
a.deleted = 0
AND a.id = $P{action_id}
LIMIT 1]]>
	</queryString>
