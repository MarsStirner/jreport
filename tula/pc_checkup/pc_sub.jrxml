-- Длина шейки матки
LEFT JOIN ActionProperty_ExtRef cervix_length ON cervix_length.id = (
  SELECT _ap.id
  FROM ActionProperty _ap
  INNER JOIN ActionPropertyType _apt ON _apt.id = _ap.type_id AND _apt.code = 'cervix_length'
  WHERE _ap.deleted = 0
  AND _apt.deleted = 0
  AND _ap.action_id = a.id
  LIMIT 1
)
-- Шейка матки
LEFT JOIN (
  SELECT id, GROUP_CONCAT(DISTINCT value) as value 
  FROM ActionProperty_ExtRef 
  GROUP BY id) cervix ON cervix.id = (
    SELECT _ap.id
