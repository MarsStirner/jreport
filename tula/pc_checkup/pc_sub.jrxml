	<queryString>
		<![CDATA[SELECT
externalia.value as externalia, -- Наружные половые органы
vagina.value as vagina,
cervix.value as cervix, -- Шейка матки
cervix_free_input.value as cervix_free_input, -- Шейка матки. Другое
cervix_length.value as cervix_length,
cervical_canal.value as cervical_canal,
cervical_canal_free_input.value as cervical_canal_free_input,
cervix_consistency.value as cervix_consistency,
cervix_position.value as cervix_position,
cervix_maturity.value as cervix_maturity,
body_of_womb.value as body_of_womb,
body_of_womb_additional.value as body_of_womb_additional,
appendages.value as appendages,
appendages_increased.value as appendages_increased,
appendages_right.value as appendages_right,
appendages_right_increased.value as appendages_right_increased,
parametrium.value as parametrium,
features.value as features, -- Особенности
vagina_secretion.value as vagina_secretion,
urethra_secretion.value as urethra_secretion,
cervical_canal_secretion.value as cervical_canal_secretion,
onco_smear.value as onco_smear,
pregnancy_week.value as pregnancy_week, -- Беременность (недель)
pregnancy_continuation.value as pregnancy_continuation, -- Возможность сохранения беременности
pregnancy_continuation_refusal.value as pregnancy_continuation_refusal, -- Отказ от прерывания
craft.value as craft,
next_date.value as next_date, -- Плановая дата следующей явки
notes.value as notes, -- Примечания
recommendations.value as recommendations, -- Рекомендации
doctor.lastName as doctorLastName, -- Ф доктора
doctor.firstName as doctorFirstName, -- И доктора
doctor.patrName as doctorPatrName -- О доктора

FROM Action a
LEFT JOIN Person doctor ON doctor.id = a.person_id
-- Цервикальный канал. Другое
LEFT JOIN ActionProperty_String cervical_canal_free_input ON cervical_canal_free_input.id= (
  SELECT _ap.id
  FROM ActionProperty _ap
  INNER JOIN ActionPropertyType _apt ON _apt.id = _ap.type_id AND _apt.code = 'cervical_canal_free_input'
  WHERE _ap.deleted = 0
  AND _apt.deleted = 0
  AND _ap.action_id = a.id
  LIMIT 1
)
-- Придаток правый. До
LEFT JOIN ActionProperty_Double appendages_right_increased ON appendages_right_increased.id= (
  SELECT _ap.id
  FROM ActionProperty _ap
  INNER JOIN ActionPropertyType _apt ON _apt.id = _ap.type_id AND _apt.code = 'appendages_right_increased'
  WHERE _ap.deleted = 0
  AND _apt.deleted = 0
  AND _ap.action_id = a.id
  LIMIT 1
)
-- Придаток левый. До
LEFT JOIN ActionProperty_Double appendages_increased ON appendages_increased.id= (
  SELECT _ap.id
  FROM ActionProperty _ap
  INNER JOIN ActionPropertyType _apt ON _apt.id = _ap.type_id AND _apt.code = 'appendages_increased'
  WHERE _ap.deleted = 0
  AND _apt.deleted = 0
  AND _ap.action_id = a.id
  LIMIT 1
)
-- Тело матки. До
LEFT JOIN ActionProperty_Double body_of_womb_additional ON body_of_womb_additional.id= (
  SELECT _ap.id
  FROM ActionProperty _ap
  INNER JOIN ActionPropertyType _apt ON _apt.id = _ap.type_id AND _apt.code = 'body_of_womb_additional'
  WHERE _ap.deleted = 0
  AND _apt.deleted = 0
  AND _ap.action_id = a.id
  LIMIT 1
)
-- Шейка матки. Другое
LEFT JOIN ActionProperty_String cervix_free_input ON cervix_free_input.id= (
  SELECT _ap.id
  FROM ActionProperty _ap
  INNER JOIN ActionPropertyType _apt ON _apt.id = _ap.type_id AND _apt.code = 'cervix_free_input'
  WHERE _ap.deleted = 0
    AND _apt.deleted = 0
  AND _ap.action_id = a.id
  LIMIT 1
)
-- Рекомендации
LEFT JOIN ActionProperty_String recommendations ON recommendations.id= (
  SELECT _ap.id
  FROM ActionProperty _ap
  INNER JOIN ActionPropertyType _apt ON _apt.id = _ap.type_id AND _apt.code = 'recommendations'
  WHERE _ap.deleted = 0
  AND _apt.deleted = 0
  AND _ap.action_id = a.id
  LIMIT 1
)
-- Примечания
LEFT JOIN ActionProperty_String notes ON notes.id= (
  SELECT _ap.id
  FROM ActionProperty _ap
  INNER JOIN ActionPropertyType _apt ON _apt.id = _ap.type_id AND _apt.code = 'notes'
  WHERE _ap.deleted = 0
  AND _apt.deleted = 0
  AND _ap.action_id = a.id
  LIMIT 1
)
-- Плановая дата следующей явки
LEFT JOIN ActionProperty_Date next_date ON next_date.id= (
  SELECT _ap.id
  FROM ActionProperty _ap
  INNER JOIN ActionPropertyType _apt ON _apt.id = _ap.type_id AND _apt.code = 'next_date'
  WHERE _ap.deleted = 0
  AND _apt.deleted = 0
  AND _ap.action_id = a.id
  LIMIT 1
)
-- Изменение условий труда
LEFT JOIN ActionProperty_ExtRef craft ON craft.id = (
  SELECT _ap.id
  FROM ActionProperty _ap
  INNER JOIN ActionPropertyType _apt ON _apt.id = _ap.type_id AND _apt.code = 'craft'
  WHERE _ap.deleted = 0
  AND _apt.deleted = 0
  AND _ap.action_id = a.id
  LIMIT 1
)
-- Отказ от прерывания
LEFT JOIN ActionProperty_Integer pregnancy_continuation_refusal ON pregnancy_continuation_refusal.id= (
  SELECT _ap.id
  FROM ActionProperty _ap
  INNER JOIN ActionPropertyType _apt ON _apt.id = _ap.type_id AND _apt.code = 'pregnancy_continuation_refusal'
  WHERE _ap.deleted = 0
  AND _apt.deleted = 0
  AND _ap.action_id = a.id
  LIMIT 1
)
-- Возможность сохранения беременности
LEFT JOIN ActionProperty_Integer pregnancy_continuation ON pregnancy_continuation.id= (
  SELECT _ap.id
  FROM ActionProperty _ap
  INNER JOIN ActionPropertyType _apt ON _apt.id = _ap.type_id AND _apt.code = 'pregnancy_continuation'
  WHERE _ap.deleted = 0
  AND _apt.deleted = 0
  AND _ap.action_id = a.id
  LIMIT 1
)
-- Беременность (недель)
LEFT JOIN ActionProperty_Integer pregnancy_week ON pregnancy_week.id= (
  SELECT _ap.id
  FROM ActionProperty _ap
  INNER JOIN ActionPropertyType _apt ON _apt.id = _ap.type_id AND _apt.code = 'pregnancy_week'
  WHERE _ap.deleted = 0
