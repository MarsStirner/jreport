  LIMIT 1
)
-- Цервикальный канал
LEFT JOIN ActionProperty_ExtRef cervical_canal ON cervical_canal.id = (
  SELECT _ap.id
  FROM ActionProperty _ap
  INNER JOIN ActionPropertyType _apt ON _apt.id = _ap.type_id AND _apt.code = 'cervical_canal'
