			<parameterDescription><![CDATA[]]></parameterDescription>
		</parameter>
		<queryString>
			<![CDATA[SELECT diag.action_id AS action_id, mkb.DiagID AS DiagID, mkb.DiagName AS DiagName, diagKind.diagnosisKind_id AS diagnosisKind_id 
FROM Diagnostic AS diag
LEFT JOIN Action_Diagnosis AS diagKind 
ON diag.action_id = diagKind.action_id AND diag.diagnosis_id = diagKind.diagnosis_id
LEFT JOIN MKB mkb ON diag.MKB = mkb.DiagID
WHERE diag.action_id = $P{action_id} AND diagKind.diagnosisKind_id = $P{diagnosisKind_id}]]>
		</queryString>
		<field name="action_id" class="java.lang.Integer"/>
		<field name="DiagID" class="java.lang.String"/>
		<field name="DiagName" class="java.lang.String"/>
		<field name="diagnosisKind_id" class="java.lang.Integer"/>
	</subDataset>
